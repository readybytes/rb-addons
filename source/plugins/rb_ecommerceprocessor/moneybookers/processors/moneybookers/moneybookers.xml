<?xml version="1.0" encoding="utf-8"?>
<config>
	<name>Moneybookers</name>
   	<author></author>
    <description></description>
   	<icon></icon>
    <tags></tags>
    <help>
    <![CDATA[
		For testing MoneyBookers Payment gateway, you need to create two accounts at MoneyBookers site and request MoneyBookers to transaform those accounts as testing accounts. <br/><br/>
		<b>It does not support Trial Subscription.</b>
	]]>
	</help>

	<fields name="processor_config">
		<fieldset name="parameters">
			<field	name="merchant_email" 			
					type="text" 
					default="" 
					label="PLG_RB_ECOMMERCEPROCESSOR_MONEYBOOKERS_PARAMS_MONEYBOOKERS_MERCHANT_EMAIL_LABEL"  
					description="PLG_RB_ECOMMERCEPROCESSOR_MONEYBOOKERS_PARAMS_MONEYBOOKERS_MERCHANT_EMAIL_DESC" 
					class="required validate-email" />
				
			<field	name="merchant_secret_word" 	
					type="text" 	
					default="" 
					label="PLG_RB_ECOMMERCEPROCESSOR_MONEYBOOKERS_PARAMS_MONEYBOOKERS_MRECHANT_SECRET_WORD_LABEL"  
					description="PLG_RB_ECOMMERCEPROCESSOR_MONEYBOOKERS_PARAMS_MONEYBOOKERS_MERCHANT_SECRET_WORD_DESC" 
					class="required" />	
					
			<field	name="password" 	
					type="text" 	
					default="" 
					label="PLG_RB_ECOMMERCEPROCESSOR_MONEYBOOKERS_PARAMS_MONEYBOOKERS_PASSWORD_LABEL"  
					description="PLG_RB_ECOMMERCEPROCESSOR_MONEYBOOKERS_PARAMS_MONEYBOOKERS_PASSWORD_DESC" 
					class="required" />	
					
			<field	name="notify_url" 	
					type="text" 	
					default="" 
					label="PLG_RB_ECOMMERCEPROCESSOR_MONEYBOOKERS_PARAMS_MONEYBOOKERS_NOTIFY_URL_LABEL"  
					description="PLG_RB_ECOMMERCEPROCESSOR_MONEYBOOKERS_PARAMS_MONEYBOOKERS_NOTIFY_URL_DESC" 
					class="required" />	
					
			<field	name="return_url" 	
					type="text" 	
					default="" 
					label="PLG_RB_ECOMMERCEPROCESSOR_MONEYBOOKERS_PARAMS_MONEYBOOKERS_RETURN_URL_LABEL"  
					description="PLG_RB_ECOMMERCEPROCESSOR_MONEYBOOKERS_PARAMS_MONEYBOOKERS_RETURN_URL_DESC" 
					class="required" />	
					
			<field	name="cancel_url" 	
					type="text" 	
					default="" 
					label="PLG_RB_ECOMMERCEPROCESSOR_MONEYBOOKERS_PARAMS_MONEYBOOKERS_CANCEL_URL_LABEL"  
					description="PLG_RB_ECOMMERCEPROCESSOR_MONEYBOOKERS_PARAMS_MONEYBOOKERS_CANCEL_URL_DESC" 
					class="required" />						
		</fieldset>
	</fields>
</config>

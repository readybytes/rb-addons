<?xml version="1.0" encoding="UTF-8"?>
<form>
<fields name="payment_data">
	<fieldset addfieldpath="/plugins/system/rbsl/rb/rb/html/fields" name="plg_rb_ecommerceprocessor_eway_form_eway_fieldset_card_details">
		<field 	name="cc_type"
				id="rb_ecommerce_processor_eway_cc_type"
				type="list"
				default=""
				label="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_CARD_TYPE_LABEL"
				description="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_CARD_TYPE_DESC"
				required="true">
				<option value="Visa">PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_CARD_TYPE_VISA</option>
				<option value="MasterCard">PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_CARD_TYPE_MASTERCARD</option>
				<option value="AmericanExpress">PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_CARD_TYPE_AMERICANEXPRESS</option>
				<option value="DinersClub">PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_CARD_TYPE_DINERSCLUB</option>
		</field>
		
		<field 	name="card_name"
				id="rb_ecommerce_processor_eway_card_name"
				type="text"
				default=""
				label="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_CARD_NAME_LABEL"
				description="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_CARD_NAME_DESC"
				required="true"/>
				
		<field 	name="card_number"
				id="rb_ecommerce_processor_eway_card_number"
				type="text"
				default=""
				label="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_CARD_NUMBER_LABEL"
				description="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_CARD_NUMBER_LABEL"
				required="true"/>
		
		<field 	name="expiration_month" 
				id="rb_ecommerce_processor_eway_expiration_month"
				type="rb_.month"
				default=""
				label="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_EXPIRATION_MONTH_LABEL"
				description="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_EXPIRATION_MONTH_DESC"
				required="true"/>
		
		<field 	name="expiration_year" 
				id="rb_ecommerce_processor_eway_expiration_year"
				type="rb_.year"
				from="now"
				for="20"
				default=""
				label="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_EXPIRATION_YEAR_LABEL"
				description="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_EXPIRATION_YEAR_DESC"
				required="true"/>				
				
		<field 	name="card_code" 
				id="rb_ecommerce_processor_eway_card_code"
				type="text"
				default=""
				maxlength="4"
				label="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_CARD_CODE_LABEL"
				description="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_CARD_CODE_LABEL"
				required="true"/>
					
	</fieldset>
	
	<fieldset addfieldpath="/plugins/system/rbsl/rb/pkg/ecommerce/html/fields" name="plg_rb_ecommerceprocessor_eway_form_eway_fieldset_customer_details">		
		
		<field 	name="name_title" 
				id="rb_ecommerce_processor_paypalpro_name_title"
				type="list"
				default=""
				label="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_TITLE_LABEL"
				description="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_TITLE_DESC">
			 	<option value="Mr.">PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_TITLE_MR</option>
               	<option value="Ms.">PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_TITLE_MS</option>
                <option value="Mrs.">PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_TITLE_MRS</option>
                <option value="Miss">PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_TITLE_MISS</option>
                <option value="Dr.">PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_TITLE_DR</option>
                <option value="Sir.">PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_TITLE_SIR</option>
                <option value="Prof.">PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_TITLE_PROF</option>			
		</field>
				
		<field 	name="company" 
				id="rb_ecommerce_processor_eway_company"
				type="text"
				default=""
				label="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_COMPANY_NAME_LABEL"
				description="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_COMPANY_NAME_DESC"/>
				
		<field 	name="first_name" 
				id="rb_ecommerce_processor_eway_first_name"
				type="text"
				default=""
				label="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_FIRST_NAME_LABEL"
				description="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_FIRST_NAME_DESC"/>
				
		<field 	name="last_name" 
				id="rb_ecommerce_processor_eway_last_name"
				type="text"
				default=""
				label="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_LAST_NAME_LABEL"
				description="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_LAST_NAME_DESC"/>
				
		<field 	name="email" 
				id="rb_ecommerce_processor_eway_email"
				type="text"
				default=""
				label="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_EMAIL_LABEL"
				description="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_EMAIL_DESC"
				required="true"/>
				
		<field 	name="phone" 
				id="rb_ecommerce_processor_eway_phone"
				type="hidden"
				default=""
				label="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_PHONE_LABEL"
				description="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_PHONE_DESC"/>
				
		<field 	name="mobile" 
				id="rb_ecommerce_processor_eway_mobile"
				type="hidden"
				default=""
				label="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_MOBILE_LABEL"
				description="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_MOBILE_DESC"/>
				
		<field 	name="fax" 
				id="rb_ecommerce_processor_eway_fax"
				type="hidden"
				default=""
				label="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_FAX_LABEL"
				description="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_FAX_DESC"/>
				
		<field 	name="address" 
				id="rb_ecommerce_processor_eway_address"
				type="hidden"
				default=""
				label="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_ADDRESS_LABEL"
				description="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_ADDRESS_DESC"/>
				
		<field 	name="city" 
				id="rb_ecommerce_processor_eway_city"
				type="text"
				default=""
				label="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_CITY_LABEL"
				description="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_CITY_DESC"/>
				
		<field 	name="state" 
				id="rb_ecommerce_processor_eway_state"
				type="text"
				default=""
				label="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_STATE_LABEL"
				description="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_STATE_DESC"/>
			
		<field 	name="country" 
				id="rb_ecommerce_processor_eway_country"
				type="list"
				default=""
				label="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_COUNTRY_LABEL"
				description="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_COUNTRY_DESC">
				<option value="AU">Australia</option>
        		<option value="NZ">New Zealand</option>
          		<option value="GB">United Kingdom</option>
         </field>
		
		<field 	name="postcode" 
				id="rb_ecommerce_processor_eway_postcode"
				type="text"
				default=""
				label="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_POST_CODE_LABEL"
				description="PLG_RB_ECOMMERCEPROCESSOR_EWAY_FORM_POST_CODE_DESC"
				required="true"/>					
	</fieldset>
</fields>
</form>